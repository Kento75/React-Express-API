  "author": "Your Name",
  "license": "MIT",
  "description": "search screen sample by react + redux + material-ui + MongoDB",
  "dependencies": {
    "axios": "^0.12.0",
    "cors": "^2.8.4",
    "express": "^4.14.0",
    "material-ui": "^0.16.3",
    "react": "^15.4.0",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.1",
    "react-router-redux": "^4.0.5",
    "react-tap-event-plugin": "^2.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "webpack-dev-middleware": "^1.6.1"
  },
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "webpack": "^1.13.1"
